#:kivy 1.10.1

<TicTacToeGame>:
    size: (1000,1100)
    FloatLayout:
        size: (root.width,root.height)
        Button:
            size_hint: (None,None)
            size: (root.width,root.height * 0.1)
            id: new_button
            text: 'New game'
            background_color: (1, 1, 0, 1)
            pos: (0, root.height * 0.9)

